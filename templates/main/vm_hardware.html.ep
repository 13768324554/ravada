{{showmachine.hardware}}
<div class="panel-body">
    <div ng-repeat="(name,value) in showmachine.hardware">
        <h2>{{name}}</h2>
        <ul>
            <li ng-repeat="item in value track by $index">{{item}} {{$index}}
                <button ng-click="test(1)"><i class="fa fa-times">a</i></button>
            </li>
        </ul>
    </div>

</div>
